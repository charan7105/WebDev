
<%- include("partials/header"); -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" placeholder="Write the Title for your Blog" type="text" name="postTitle">
    </div>
    <div class="form-group">
    <label>Post</label>
    <textarea class="form-control"  placeholder="Haha, Enjoy by writing your own blog" name="postBody" rows="5" cols="30"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer"); -%>
